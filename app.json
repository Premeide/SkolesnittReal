{
  "expo": {
    "name": "SkoleSnitt",
    "slug": "SkoleSnitt",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/skoleLogo.png",
    "splash": {
      "image": "./src/assets/images/skoleLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.SkoleSnitt",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}
